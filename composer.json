{
    "name": "yuloh/container",
    "type": "library",
    "description": "A lightweight container-interop compatible DI container",
    "keywords": [
        "yuloh",
        "container"
    ],
    "homepage": "https://github.com/yuloh/container",
    "license": "MIT",
    "authors": [
    ],
    "require": {
        "php" : ">=5.4.0",
        "psr/container": "^1.0"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Yuloh\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuloh\\Container\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs --standard=psr2 src/"
    }
}
